{
  "name": "boxel-3d-mod-loader",
  "version": "0.0.1",
  "description": "A mod loader for Boxel 3D",
  "scripts": {
    "build:min": "vite build --mode minify",
    "build:full": "vite build --mode unminify",
    "build": "npm run build:full && npm run build:min",
    "build:ext": "vite build --mode extension && vite build --mode extension-content && cp extension/manifest.json extension/dist/",
    "dev": "vite dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "matter-js": "^0.20.0",
    "semver": "^7.7.3",
    "svelte": "^5.46.0",
    "svelte-portal": "^2.2.1",
    "three": "^0.181.2",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/matter-js": "^0.20.2",
    "@types/semver": "^7.7.1",
    "@types/three": "^0.181.0",
    "vite": "^7.2.6"
  }
}
